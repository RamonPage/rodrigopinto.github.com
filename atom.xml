---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>rodrigopinto dot me</title>
 <link href="http://rodrigopinto.me/atom.xml" rel="self"/>
 <link href="http://rodrigopinto.me/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://rodrigopinto.me/</id>
 <author>
   <name>Rodrigo Pinto</name>
   <email>rodrigopinto@gmail.com</email>
 </author>
 
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://rodrigopinto.me{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://rodrigopinto.github.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>